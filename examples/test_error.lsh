func toggleDivisionByZero :: bool m => num (
    if(m,
        /(3,0),
        /(3,1)
    )
)

func toggleEmptyList :: bool m => num (
    if(m,
        head([num]),
        head([1,2,3,4,5,6,8])
    )
)

func map :: [num] l, (num => num) f => [num] (
    if (empty(l),
        [0],
        insert(f(head(l)), map(tail(l), f))
    )
)

